<!doctype html>
<html>
  <head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>表单</title>
    <script type="text/javascript" src="/allsites/js/jQuery-v1.9.0.js"></script>
    <script type="text/javascript" src="/allsites/js/template_main.js"></script>
    <link href="/allsites/style/template_style.css" rel="stylesheet" type="text/css"/>
  </head>

  <body class="qrCode" style="background-image: url(<%= @qr_code.img_path %>);">
  <article>
    <section>
      <div class="qrCodeBox">
        <div class="qrCodePrize">
          <img src="<%= @qr_img %>" style="width:inherit"/>
        </div>
      </div>
      <div class="qrCodeInfo">
        <span><%= @code %></span>
      </div>
    </section>
  </article>
  <%= raw page_footer(@site) %>
</body>

</html>
